@page
@model LoginModel
@{
    ViewData["Title"] = "Food2U Login";
}
<p>Login Page!</p>
<form method="post">
  <div class="form-group">
    <label asp-for="Credential.Username">Username</label>
    <input type="text" asp-for="Credential.Username" class="form-control" required/>
  </div>

  <div class="form-group">
    <label for="Credential.Password">Password</label>
    <input type="text" asp-for="Credential.Password" class="form-control" required/>
  </div>

  <div class="form-group">
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
</form>